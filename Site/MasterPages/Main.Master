    <ext:Window ID="AboutWindow" runat="server" Icon="Information" Title="关于..." Height="260"
        Width="400" Collapsible="false" Closable="false" Modal="true" InitCenter="true" Resizable="false"
        Hidden="true">
        <Content>
            <table border="0" cellpadding="0" cellspacing="0" style="width:100%; height:auto;">
