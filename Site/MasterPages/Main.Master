                <Listeners>
                    <Click Handler="#{AboutWindow}.hide();" />
                </Listeners>
            </ext:Button>
        </Buttons>
    </ext:Window>
    <ext:Window ID="VideoWindow" runat="server" Width="400" Height="220" Modal="true" Hidden="true" Layout="FitLayout">
        <Items>
            <ext:FormPanel ID="VideoFormPanel" runat="server" Header="false" Padding="10" ButtonAlign="Right" MonitorValid="true">
                <Items>
                    <ext:TextField ID="VideoIPField" runat="server" FieldLabel="平台地址" AnchorHorizontal="95%" MaxLength="100" AllowBlank="false" EmptyText="示例: http://192.168.1.1" />
                    <ext:SpinnerField ID="VideoPortField" runat="server" FieldLabel="平台端口" AnchorHorizontal="95%" MinValue="1" MaxValue="65535" AllowDecimals="false" IncrementValue="1" AllowBlank="false"/>
                    <ext:TextField ID="VideoUIDField" runat="server" FieldLabel="登录账户" AnchorHorizontal="95%" AllowBlank="false" MaxLength="50" />
                    <ext:TextField ID="VideoPwdField" runat="server" FieldLabel="登录密码" AnchorHorizontal="95%" AllowBlank="false" MaxLength="50" InputType="Password" />
                </Items>
                <Buttons>
                    <ext:Button ID="SaveVideoBtn" runat="server" Text="保存">
                        <Listeners>
                            <Click Handler="if (#{VideoFormPanel}.getForm().isValid()) {
                                #{VideoStatusBar}.setStatus({text: LanguageSet.Saving, iconCls: 'icon-loading'});
                                X.SaveVideoParamters({
                                    success : function (result) {
                                        if(!Ext.isEmpty(result, false)){
                                            var msg = Ext.util.JSON.decode(result, true);
                                            if(msg) {
                                                if(msg.Status == 200){
                                                    #{VideoStatusBar}.setStatus({text: msg.Msg, iconCls: 'icon-tick'});
                                                } else{ 
                                                    #{VideoStatusBar}.setStatus({text: msg.Msg, iconCls: 'icon-exclamation'}); 
                                                }
                                            }
                                        }
                                    }
                                });
                            } else { 
                                #{VideoStatusBar}.setStatus({text: LanguageSet.ErrorForm, iconCls: 'icon-exclamation'}); 
                            }" />
                        </Listeners>
                    </ext:Button>
                    <ext:Button ID="CancelVideoBtn" runat="server" Text="取消">
                        <Listeners>
                            <Click Handler="#{VideoWindow}.hide();" />
                        </Listeners>
                    </ext:Button>
                </Buttons>
                <BottomBar>
                    <ext:StatusBar ID="VideoStatusBar" runat="server" Height="25" />
                </BottomBar>
            </ext:FormPanel>
        </Items>
    </ext:Window>
    <ext:Window ID="AlmExceptionParamWindow" runat="server" Icon="TableEdit" Title="异常告警参数配置" Height="240" Width="400" Modal="true" InitCenter="true" Hidden="true" BodyStyle="background:#fff;">
        <Items>
            <ext:FormPanel ID="AlmExceptionParamFormPanel" runat="server" Border="false" LabelWidth="180" Padding="10" ButtonAlign="Right" MonitorValid="true">
                <Items>
                    <ext:TextField ID="AlmExTextField1" runat="server" FieldLabel="重要告警标识" AnchorHorizontal="95%" MaxLength="100" AllowBlank="false" Text="#ZYGJ" />
                    <ext:SpinnerField ID="AlmExSpinnerField1" runat="server" FieldLabel="超频告警阈值(次数)" AnchorHorizontal="95%" MinValue="1" AllowDecimals="false" IncrementValue="1" AllowBlank="false" Number="10" />
                    <ext:SpinnerField ID="AlmExSpinnerField2" runat="server" FieldLabel="超短告警阈值(秒钟)" AnchorHorizontal="95%" MinValue="1" AllowDecimals="false" IncrementValue="1" AllowBlank="false" Number="10"/>
                    <ext:SpinnerField ID="AlmExSpinnerField3" runat="server" FieldLabel="超长告警阈值(秒钟)" AnchorHorizontal="95%" MinValue="1" AllowDecimals="false" IncrementValue="1" AllowBlank="false" Number="3600"/>
                    <ext:SpinnerField ID="AlmExSpinnerField4" runat="server" FieldLabel="发现时间异常告警阈值(秒钟)" AnchorHorizontal="95%" MinValue="1" AllowDecimals="false" IncrementValue="1" AllowBlank="false" Number="1800"/>
                    <ext:SpinnerField ID="AlmExSpinnerField5" runat="server" FieldLabel="清除时间异常告警阈值(秒钟)" AnchorHorizontal="95%" MinValue="1" AllowDecimals="false" IncrementValue="1" AllowBlank="false" Number="1800"/>
                </Items>
            </ext:FormPanel>
        </Items>
        <Buttons>
            <ext:Button ID="OkButton" runat="server" Text="确定">
                <DirectEvents>
                    <Click Before="return #{AlmExceptionParamFormPanel}.getForm().isValid();" OnEvent="OkButton_Click"/>
                </DirectEvents>
            </ext:Button>
            <ext:Button ID="CancelButton" runat="server" Text="取消">
                <Listeners>
                    <Click Handler="#{AlmExceptionParamWindow}.hide();" />
                </Listeners>
            </ext:Button>
        </Buttons>
    </ext:Window>
    <ext:Window ID="GroupNodeDisplayWindow" runat="server" Icon="TableEdit" Title="群组节点参数配置" Height="220" Width="320" Modal="true" InitCenter="true" CloseAction="Hide" Hidden="true" BodyStyle="background:#fff;">
        <Items>
            <ext:FormPanel ID="GroupNodeDisplayFormPanel" runat="server" Border="false" LabelWidth="100" Padding="10" ButtonAlign="Right" MonitorValid="true">
                <Items>
                    <ext:CheckboxGroup ID="DisplayCheckboxGroup" runat="server" ColumnsNumber="1" FieldLabel="局站节点参数">
                        <Items>
                            <ext:Checkbox ID="middisplay" runat="server" BoxLabel="显示局站MID" InputValue="1" />
                            <ext:Checkbox ID="stafeaturesdisplay" runat="server" BoxLabel="显示局站特征" InputValue="2"  />
                            <ext:Checkbox ID="statypedisplay" runat="server" BoxLabel="显示局站类型" InputValue="4"  />
                        </Items>
                    </ext:CheckboxGroup>
                </Items>
            </ext:FormPanel>
        </Items>
        <Buttons>
            <ext:Button ID="SettingButton" runat="server" Text="确定">
                <DirectEvents>
                    <Click Before="return #{GroupNodeDisplayFormPanel}.getForm().isValid();" OnEvent="SettingButton_Click"/>
                </DirectEvents>
            </ext:Button>
            <ext:Button ID="CloseButton" runat="server" Text="取消">
                <Listeners>
                    <Click Handler="#{GroupNodeDisplayWindow}.hide();" />
                </Listeners>
            </ext:Button>
        </Buttons>
    </ext:Window>
    </form>
